<% include header.ejs %>

<main role="main" id="site-content">
    <div class="page-container page-container-responsive row-space-top-8">
        <div class="row row-condensed row-table row-space-4" id="receipt-id">

            <div class="col-3 col-bottom">
            </div>
            <div class="col-6 col-bottom text-right">
            </div>
        </div>

        <div class="row">
            <div class="panel">
                <div class="panel-body">
                    <h2>Customer Receipt</h2>

                    <h5 style="margin-top:-15px !important;">CONFIRMATION CODE: 4TOQ5I</h5>


                   <div class="pull-right hide-print">
                       <div style="margin-top:-70px;font-family: 'Courier New'">
                            AIRBNB<br>
                        +1 (415) 800-5959<br>
                        WWW.AIRBNB.COM/CONTACT
                       </div>
                        <a style="margin-top:10px;" id="print_receipt" onclick="print_receipt()" class="btn" href="#">Receipt</a>
                    </div>

                    <div class="h6 row-space-1">
                        <%= new Date(data[0].createdDate).toDateString() %><br>
                        RECEIPT # <%= data[0].billingId %>
                    </div>
                    <div class="h4">

                    </div>
                </div>

                <div class="panel-body">

                    <div class="row row-space-condensed row-space-3">
                        <div class="col-3">
                            <h6>
                                Name
                            </h6>
                            <p class="text-lead">
                               <strong> <%= data[0].userId.firstName %> <%= data[0].userId.lastName %></strong>
                            </p>
                        </div>
                        <div class="col-3">
                            <h6>
                                Travel Destination
                            </h6>
                            <p class="text-lead">
                                <b><%= data[0].propertyId.city %> <%= data[0].propertyId.state %> <%= data[0].propertyId.country %></b>
                            </p>
                        </div>
                        <div class="col-3">
                            <h6>
                                Nights
                            </h6>
                            <p class="text-lead">
                                <strong><%= Math.floor(data[0].total / data[0].propertyId.price) %></strong>
                            </p>
                        </div>
                        <div class="col-3">
                            <h6>
                                Accommodation Type
                            </h6>
                            <p class="text-lead">
                                <strong><%= data[0].propertyId.category %></strong>
                            </p>
                        </div>
                    </div>

                    <div class="row row-space-condensed">
                        <div class="col-3">
                            <h6>
                                Accommodation Address
                            </h6>
                            <p class="text-lead">
                                <strong><%= data[0].propertyId.address %><br>
                                    <strong><%= data[0].propertyId.city %> <%= data[0].propertyId.state %>
                                        <br><%= data[0].propertyId.country %><br></strong>
                                </strong>

                            </p>

                        </div>
                        <div class="col-3">
                            <h6>
                                Accommodation Host
                            </h6>
                            <p class="text-lead">
                               <strong> <%= data[0].hostId.firstName %> <%= data[0].hostId.lastName %></strong>
                            </p>
                        </div>
                        <div class="col-3">
                            <h6>
                                Check In
                            </h6>
                            <p class="text-lead">
                                <strong><%= new Date(data[0].fromDate).toLocaleDateString() %><br>12:00 PM (noon)</strong>
                            </p>
                        </div>
                        <div class="col-3">
                            <h6>
                                Check Out
                            </h6>
                            <p class="text-lead">
                                <strong> <%= new Date(data[0].toDate).toLocaleDateString() %><br>12:00 PM (noon)</strong>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="row row-space-condensed row-space-top-4">
                        <div class="col-12">
                            <h2>
                                Payment Details
                            </h2>

                            <table class="table table-bordered payment-table">
                                <tbody>
                                <tr>
                                    <th class="receipt-label"><%= Math.floor(data[0].total / data[0].propertyId.price) %>
                                        Nights
                                    </th>
                                    <td class="receipt-amount">
                                        &#36;<%= (data[0].propertyId.price *Math.floor(data[0].total / data[0].propertyId.price)) %> ( $<%= data[0].propertyId.price %> Per Night)
                                    </td>
                                </tr>
                                <tr>
                                    <th class="receipt-label">
                                        Dynamic Price (<%=data[0].propertyId.multiplier%>X)
                                    </th>
                                    <td class="receipt-amount">$<%= (data[0].total)%></td>
                                </tr>
                                <tr>
                                    <th class="receipt-label">
                                        Airbnb Service Fee
                                    </th>
                                    <td class="receipt-amount">&#36;35</td>
                                </tr>
                                <tr>
                                    <th class="receipt-label">
                                        Airbnb Concierge <sup style="font-size: 10px ">BETA </sup>
                                    </th>
                                    <td class="receipt-amount">Free</td>
                                </tr>
                                </tbody>
                                <tfoot>
                                <tr>
                                    <th class="receipt-label" style="text-align: right;">Total</th>
                                    <td class="receipt-amount">&#36;<%= (data[0].total)+35 %></td>
                                </tr>
                                </tfoot>

                            </table>

                            <table class="table table-bordered payment-table">
                                <tbody>
                                <tr>
                                    <th class="receipt-label">
                                        Payment Received:
                                        <%= new Date(data[0].createdDate).toString() %>

                                    </th>
                                    <td class="receipt-amount">

                                        &#36;<%= (data[0].total)+35 %>
                                    </td>
                                </tr>

                                </tbody>
                            </table>

                            <br>

                            <div style="width:100%;margin:auto;" id="barcode">
                                123456
                            </div>
                            <div style="font-family:Courier New;width:100%;margin:auto;text-align: center;">
                                THANKS FOR TRAVELLING WITH AIRBNB
                            </div>



                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div style="font-family:Courier New;" class="row-space-top-4" id="legal-disclaimer">
            <p>
                Airbnb is authorized to accept Accommodation Fees on behalf of the Host as its limited agent. This means
                that your payment obligation to the Host is satisfied by your payment to Airbnb. Any disagreements by
                the Host regarding that payment must be settled between the Host and Airbnb.
            </p>
        </div>
    </div>

</main>

<script>
    function print_receipt() {
        window.print();
    }
</script>




