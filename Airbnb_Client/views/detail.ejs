                                                            <input ng-model="bidAmount"
                                                                   class="checkin"
                                                                   autocomplete="off" id="bid" name="bid"
                                                                  ng-value="{{room_result.rooms_price.maxBidPrice}}" type="number" min="{{room_result.rooms_price.maxBidPrice+1}}">
                                                            </br>
                                                            <label for="bid">
                                                                Start Date : {{room_result.startDate | date : 'mediumDate'}}
                                                            </label>

                                                            <label for="bid">
                                                                End Date : {{room_result.endDate | date : 'mediumDate'}}
                                                            </label>
                            </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-1"></div>
                                            </div>
                                        </div>
                                        <div class="js-book-it-status">
                                            <div class="js-book-it-enabled clearfix">
                                                <div class="js-book-it-btn-container ">
                                                    <button type="button"
                                                            ng-click="placeBid(bidAmount)"
