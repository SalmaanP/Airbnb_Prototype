                                    <span ng-if="!room_result.isBidding"id="rooms_price_amount" value=""> {{room_result.rooms_price.night}}X {{room_result.rooms_price.multiplier}}=${{nightPrice}}</span>
                                    <span ng-if="room_result.isBidding" id="rooms_price_amount" value=""> {{room_result.rooms_price.maxBidPrice}}X {{room_result.rooms_price.multiplier}}=${{nightPrice}}</span>

                                </div>
                                <i class="icon icon-bolt icon-beach pull-left h3 pricing__bolt"></i>

                                <div id="payment-period-container" class="pull-right">
                                    <div id="per_night" class="per-night">
                                        Per Night
                                    </div>

                                    <div id="per_month" class="per-month hide">
                                        Per Month
